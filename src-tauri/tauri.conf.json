{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "digi-angular",
  "version": "0.1.0",
  "identifier": "digi-angular",
  "build": {
    "frontendDist": "../dist/digi-angular/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "npm run web",
    "beforeBuildCommand": "npm run build:angular"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Digi Angular",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
